{
    "name": "kekke88/phpdantic",
    "description": "Data validation using PHP type hints.",
    "autoload": {
        "psr-4": {
            "Kekke88\\Phpdantic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kekke88\\PhpdanticTest\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "squizlabs/php_codesniffer": "3.*"
    },
    "require": {
        "php": "^7.4||^8.0",
        "roave/better-reflection": "^6.16"
    },
    "authors": [
        {
            "name": "Henric Johansson",
            "email": "hej@henricjohansson.se",
            "homepage": "https://henricjohansson.se",
            "role": "Developer"
        }
    ],
    "license": "Apache-2.0"
}
