{
    "name": "kekke88/phpdantic-package",
    "description": "Data validation using PHP type hints.",
    "autoload": {
        "psr-4": {
            "Kekke88\\Phpdantic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kekke88\\PhpdanticTest\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "squizlabs/php_codesniffer": "3.*"
    },
    "require": {
        "roave/better-reflection": "^6.16"
    }
}
